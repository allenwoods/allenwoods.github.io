&amp;#x7531;&amp;#x4E8E;&amp;#x56FD;&amp;#x5185;&amp;#x7F51;&amp;#x7EDC;&amp;#x7684;&amp;#x539F;&amp;#x56E0;, &amp;#x4E0B;&amp;#x8F7D;&amp;#x865A;&amp;#x62DF;&amp;#x955C;&amp;#x50CF;&amp;#x6587;&amp;#x4EF6;&amp;#x901F;&amp;#x5EA6;&amp;#x5947;&amp;#x6162;, &amp;#x5EFA;&amp;#x8BAE;&amp;#x4F7F;&amp;#x7528;&amp;#x5176;&amp;#x4ED6;&amp;#x65B9;&amp;#x5F0F;&amp;#x4E0B;&amp;#x8F7D;&amp;#x3002;
&amp;#x5047;&amp;#x8BBE;&amp;#x4E0B;&amp;#x8F7D;&amp;#x540E;&amp;#x6587;&amp;#x4EF6;&amp;#x5728;&amp;#x5F53;&amp;#x524D;&amp;#x5B89;&amp;#x88C5;&amp;#x76EE;&amp;#x5F55;&amp;#x4E0B;, &amp;#x6587;&amp;#x4EF6;&amp;#x540D;&amp;#x4E3A; &lt;code&gt;dst.box&lt;/code&gt;
